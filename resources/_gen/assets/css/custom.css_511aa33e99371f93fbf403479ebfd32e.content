body{margin:0;background-color:#dbcfb2}body .container{max-width:800px;margin:0 auto}nav{border-bottom:1px solid rgba(119,87,64,.2)}nav li a{filter:drop-shadow(4px 4px 7px #774E2A)}nav li a:hover{text-decoration:underline}article.image_summary h1 a{color:#774e2a;font-weight:700}article.image_summary .summary{color:#906e49}article.image_summary .article-footer{color:#906e49;margin-top:8px}article.image_summary .article-footer .article-info-box{display:flex;align-items:center;justify-content:space-between}article.image_summary .article-footer .article-info-box .post-tags{display:flex;align-items:center}footer a{color:rgba(90,90,90,.7)!important}.post-article header{color:#774e2a}.post-article .article-body{color:#906e49}